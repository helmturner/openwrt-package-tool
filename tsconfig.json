{
    "include": ["**/*.ts"],
    "compilerOptions": {
      "strict": true,
      "target": "es6",
      "module": "commonjs",
      "sourceMap": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "allowImportingTsExtensions": true,
      "noEmit": true,
      "resolveJsonModule": true
    }
  }
  